<?xml version="1.0" encoding="utf-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    version     = "3.1.0" >

  <!-- versionCode is optional and Android only -->

  <name>AveryScott</name>

  <description>
    Your Office Rental Services Ghana
  </description>

  <author href="https://build.phonegap.com" email="info@simodigital.net">
    Gah
  </author>
  <feature name="SocialSharing">
    <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
  </feature>
  <feature name="Geolocation">
    <param name="android-package" value="org.apache.cordova.GeoBroker" />
  </feature>
  <icon src="AppResource/Android/drawable-ldpi/ldpi.png" gap:platform="android" gap:density="ldpi" />
  <icon src="AppResource/Android/drawable-mdpi/mdpi.png" gap:platform="android" gap:density="mdpi" />
  <icon src="AppResource/Android/drawable-hdpi/hdpi.png" gap:platform="android" gap:density="hdpi" />
  <icon src="AppResource/Android/drawable-xhdpi/xhdpi.png" gap:platform="android" gap:density="xhdpi" />
   <access origin="*" />
  <gap:plugin name="nl.x-services.plugins.socialsharing" />
 
</widget>